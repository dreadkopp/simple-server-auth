{
  "name": "dreadkopp/simple-service-auth",
  "description": "provides a simple way to allow access to services via a shared token",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/auth": "^9|^10",
    "illuminate/support": "^9|^10",
    "illuminate/routing": "^9|^10"
  },
  "autoload": {
    "psr-4": {
      "dreadkopp\\SimpleServiceAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "dreadkopp\\SimpleServiceAuth\\Tests\\" : "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.5"
  }
}
